<!DOCTYPE html>  

 <html>
    
     <head>
        
        
        
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <title>{% block title %}Filmstrip{% endblock %}</title>
               
               {% block stylesheets %}{% endblock %}
               
               <link href="{{ asset('css/streetpics.css') }}" rel="stylesheet" />        
             
    
    
    </head>
    
    
    <body>
     
        
        
        
        <div>
	        {% block dazfilmstrip %}
	        
	        <h1 class = "body">dazfilmstrip_block in base html</h1>
	        
	        {% endblock %}
        </div>
        
        
        
        
        
        
        <div id="sidebar">
            {% block sidebar %}
                <ul>
                      <li><a href="/">Home</a></li>
                      <li><a href="/blog">Blog</a></li>
                </ul>
            {% endblock %}
        </div>
        
        
        
        
        
        
        
        <div id="content">
            {% block body %}{% endblock %}
        </div>
        
        
        
        
        
        
        {% block javascripts %}
            <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
            <script src="{{ asset('/js/index.js') }}"></script>
        {% endblock %}
        
    
    
    
    </body>

</html>